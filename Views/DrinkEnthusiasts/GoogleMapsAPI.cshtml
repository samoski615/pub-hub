@{
    ViewData["Title"] = "GoogleMapAPI";
}
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie-edge">
    <title> My Google Map</title>
    <style>
        #map {
            height: 400px;
            width: 400px;
        }

        html, body {
            height: 400%;
            margin: 0;
            padding: 0;
        }
    </style>
</head>
<body>
    <h2>GoogleMapAPI</h2>
    <div id="map"></div>
    <script>

        var map;


        function initMap() {
            var dcc = {lat: 43.034271, lng:-87.911919}
            map = new google.maps.Map(document.getElementById('map'), {
                center: { lat: 43.0389, lng: -87.9065 },
                zoom: 8,
            });
            var marker = new google.maps.Marker({ position: dcc, map: map });
        }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAY94W50Ro6315b5RFNT3TTUKMd3DsUrEU&callback=initMap"
            async defer></script>

</body>
</html>




